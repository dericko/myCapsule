<h2>Users</h2>

<table>
	<tr>
		<td><p>name<p></td>
		<td>email</td>
		<td>password</td>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= link_to user.name, user_path(user.id) %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to "Edit", edit_user_path(user.id) %></td>
		<td><%= link_to "Delete", user, confirm: "Delete this user?", method: :delete %></td>
	</tr>
	<% end %>
</table>