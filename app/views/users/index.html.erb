<h1>I'm an ugly Users#index page</h1>
<p>Find me in app/views/users/index.html.erb</p>

<br>
<%= link_to "Home", root_path %>
<%= link_to "Add User", new_user_path %>
<br>

<table>
	<tr>
		<td>name</td>
		<td>email</td>
		<td>password</td>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= link_to user.name, user_path(user.id) %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to "Edit", edit_user_path(user.id) %></td>
		<td><%= link_to "Delete", user, confirm: "Delete this user?", method: :delete %></td>
	</tr>
	<% end %>
</table>